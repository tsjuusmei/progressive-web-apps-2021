<%- include('./components/head') -%>

  <header>
    <h1>Mars Rover Photos</h1>

    <h2>
      Average temparature on Mars: <%= averageTemp %> Â°C
    </h2>
  </header>

  <main>
    <ul>
      <% solData.forEach(item=> { %>
        <li>
          <a href="/sol/<%=item.photos[0].sol%>">
            <img src="<%= item.photos[0].img_src %>" alt="">
            <p>
              SOL: <%= item.photos[0].sol %> <br> <br> <br>
                Earth date: <%= item.photos[0].earth_date %> <br> <br> <br>
                  Click for more pictures
            </p>
          </a>
        </li>
        <% }) %>
    </ul>
  </main>



  <%- include('./components/footer') -%>